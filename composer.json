{
    "name": "slam/phpactor-type-inference-extension",
    "description": "PHPActor Type inference extension",
    "license": "MIT",
    "type": "phpactor-extension",
    "authors": [
        {
            "name": "Filippo Tessarotto",
            "email": "zoeslam@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "phpactor/container": "^2.0.0",
        "phpactor/worse-reflection-extension": "^0.2.4",
        "microsoft/tolerant-php-parser": "^0.0.23 || ^0.1.0"
    },
    "require-dev": {
        "malukenho/mcbumpface": "^1.1.5",
        "phpunit/phpunit": "^9.5.6",
        "phpactor/test-utils": "^1.1.3",
        "psr/container": "^1.1.1"
    },
    "autoload": {
        "psr-4": {
            "SlamPhpactor\\Extension\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SlamPhpactor\\Extension\\Tests\\": "tests/"
        }
    },
    "extra": {
        "phpactor.extension_class": "SlamPhpactor\\Extension\\TypeInferenceExtension",
        "mc-bumpface": {
            "stripVersionPrefixes": true
        }
    }
}
